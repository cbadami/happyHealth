<body>
    <div class="app-container app-theme-white body-tabs-shadow fixed-sidebar fixed-header">
        <div class="app-header header-shadow">
            <div class="app-header__logo">
                <!-- <img class="logo-src" src="/static/assets/images/happyHealth/Artboard.png" alt="image not found"
                    onclick="window.location.href = '/adminHome'"> -->
                <!-- <div class="h3 m-0 ml-2">HappyHealth</div> -->
                <div class="header__pane ml-auto">
                    <div>
                        <button type="button" class="hamburger close-sidebar-btn hamburger--elastic is-active"
                            data-class="closed-sidebar">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
            </div>
            <div class="app-header__mobile-menu">
                <div class="d-flex app-header__menu">
                    <button type="button" class="btn btn-transparent mobile-toggle-nav">
                        <i class="fa fa-bars fa-2x" aria-hidden="true"></i>
                    </button>
                    <!-- <form action="">
                        <input type="date">
                    </form> -->
                </div>
            </div>
            <div class="app-header__menu">
                <ul class="header-menu nav align-items-center"">
                    <li class="nav-item" title="Profile">
                        <p><%= userName %></p>
                    </li>
                    <li class="nav-item">
                        <a href="/adminInfo" class="nav-link">
                            <i class="fa fa-user-circle-o fa-2x" aria-hidden="true"></i>
                        </a>
                    </li>
                    <li class="btn-group nav-item">
                        <a href="/logout" class="nav-link">
                            <i class="fa fa-sign-out fa-2x" aria-hidden="true"></i>
                        </a>
                    </li>
                </ul>
            </div>
            <div class="app-header__content">
                <div class="app-header-left">
                    <!-- <form action="">
                        <input type="date">
                    </form> -->
                </div>
                <div class="app-header-center h2">
                    FitNest
                </div>
                <div class="app-header-right ">
                    <ul class="header-menu nav align-items-center"">
                        <li class="nav-item" title="Profile">
                            <p><%= userName %></p>
                        </li>
                        <li class="nav-item">
                            <a href="/adminInfo" class="nav-link">
                                <i class="fa fa-user-circle-o fa-2x" aria-hidden="true"></i>
                            </a>
                        </li>
                        <li class="btn-group nav-item">
                            <a href="javascript: window.location.href='/logout'" class="nav-link">
                                <i class="fa fa-sign-out fa-2x" aria-hidden="true"></i>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="app-main">
            <div class="app-sidebar sidebar-shadow">
                <div class="app-header__logo">
                    <img class="logo-src" src="/static/assets/images/happyHealth/Artboard.png" alt="">
                    <div class="header__pane ml-auto">
                        <div>
                            <button type="button" class="hamburger close-sidebar-btn hamburger--elastic"
                                data-class="closed-sidebar">
                                <span class="hamburger-box">
                                    <span class="hamburger-inner"></span>
                                </span>
                            </button>
                        </div>
                    </div>
                </div>
                <div class="app-header__mobile-menu">
                    <div>
                        <button type="button" class="hamburger hamburger--elastic mobile-toggle-nav">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
                <div class="scrollbar-sidebar">
                    <div class="app-sidebar__inner">
                        <ul class="vertical-nav-menu my-4">
                            <li>
                                <a href="javascript: window.location.href='/adminHome'" class="mm-active">
                                    <i class="fa fa-home" aria-hidden="true"></i>
                                    <span>Dashboard</span>
                                </a>
                            </li>
                            <li>
                                <a href="javascript: window.location.href='/userManagement';" class="mm-active">
                                    <i class="fa fa-user" aria-hidden="true"></i>
                                    <span>Users</span>
                                </a>
                            </li>
                            <!-- <li>
                                <a href="javascript: window.location.href='/groupManagement';" class="mm-active">
                                    <i class="fa fa-users" aria-hidden="true"></i>
                                    <span>Groups</span>
                                </a>
                            </li> -->
                            <li>
                                <a href="javascript: window.location.href='/challengeManagement'" class="mm-active">
                                    <i class="fa fa-file-text" aria-hidden="true"></i>
                                    <span>Challenges</span>
                                </a>
                            </li>
                            <li>
                                <a href="javascript: window.location.href='/adminAnalytics'" class="mm-active">
                                    <i class="fa fa-pie-chart" aria-hidden="true"></i>
                                    <span>Analytics</span>
                                </a>
                            </li>
                            <li>
                                <a href="javascript: window.location.href='/adminAnnouncements'" class="mm-active">
                                    <i class="fa fa-bell" aria-hidden="true"></i>
                                    <span>Announcements</span>
                                </a>
                            </li>
                            <li>
                                <a href="javascript: window.location.href='/logout'" class="mm-active">
                                    <i class="fa fa-sign-out" aria-hidden="true"title="Logout"></i>
                                    <span>Logout</span>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="app-main__outer">
                <main>
                    <%- body %>
                </main>
            </div>

        </div>
        <div class="app-wrapper-footer">
            <div class="app-footer">
                <div class="copyright text-center bg-white py-4">
                    <i class="fa fa-copyright"></i> Copyright 2020, made by FitNest
                </div>
            </div>
        </div>
    </div>
