        .card {
            /* width: 250px; */
            height: 250px;
            border-radius: 20px;
            box-shadow: 12px 12px 12px;
            margin: 10px 15px;
            box-sizing: border-box;
            cursor: pointer;
            transition: all 0.50s ease;
        }

        /* .card:hover {
            transform: scale(1.1, 1.1);
        } */

        img {
            display: block;
            margin-left: auto;
            margin-right: auto;
            /* width: 50%; */
            border-radius: 50%;
            border-bottom-left-radius: 30px;
            border-top-left-radius: 30px;
            border-bottom-right-radius: 30px;
            border-top-right-radius: 20px;
            width: 160px;
            height: 160px;
            padding: 5px;
        }

        .btn {
            display: block;
            margin-left: auto;
            margin-right: auto;
        }

        h4 {
            padding-top: 0.5rem;
            text-align: center;
