<div class="app-main__inner">

    <h2> User metrics between <%= startDate %> and <%= endDate %> </h2>

    
    <div class="col-md-8">
        <div class="card mb-3" style="height:auto;">
            <div class="card-body">
               
                <div class=" row">
                    <div class="col-sm-8">
                        <h6 class="mb-0">Step count:</h6>
                    </div>
                    <div class="col-sm-3 text-secondary">
                        <%if(result[0].total == null) {%>
                            <%= 0 %>
                        <%}
                        else {%>
                            <%= result[0].total %>
                        <%}%>   
                    </div>
                </div>
                <hr>
                <div class=" row">
                    <div class="col-sm-8">
                        <h6 class="mb-0">Sleep count:</h6>
                    </div>
                    <div class="col-sm-3 text-secondary">
                        <%if(result[0].totalSleep == null) {%>
                            <%= 0 %>
                        <%}
                        else {%>
                            <%= result[0].totalSleep %>
                        <%}%>
                    </div>
                </div>               
                <hr>
                <div class=" row">
                    <div class="col-sm-8">
                        <h6 class="mb-0">Water consumed:</h6>
                    </div>
                    <div class="col-sm-3 text-secondary">
                        <%if(result[0].totalWater == null) {%>
                            <%= 0 %>
                        <%}
                        else {%>
                            <%= result[0].totalWater %>
                        <%}%>
                    </div>
                </div>
                              
                <hr>
                <div class=" row">
                    <div class="col-sm-8">
                        <h6 class="mb-0">Mindful minutes:</h6>
                    </div>
                    <div class="col-sm-3 text-secondary">
                        <%if(result[0].totalMe == null) {%>
                            <%= 0 %>
                        <%}
                        else {%>
                            <%= result[0].totalMe %>
                        <%}%>
                    </div>
                </div>
                <hr>
                <div class=" row">
                    <div class="col-sm-8">
                        <h6 class="mb-0">Fruits count:</h6>
                    </div>
                    <div class="col-sm-3 text-secondary">
                        <%if(result[0].totalFruits == null) {%>
                            <%= 0 %>
                        <%}
                        else {%>
                            <%= result[0].totalFruits %>
                        <%}%>
                    </div>
                </div>
                
                <hr>
                <div class=" row">
                    <div class="col-sm-8">
                        <h6 class="mb-0">Vegetables count:</h6>
                    </div>
                    <div class="col-sm-3 text-secondary">
                        <%if(result[0].totalVeggies == null) {%>
                            <%= 0 %>
                        <%}
                        else {%>
                            <%= result[0].totalVeggies %>
                        <%}%>
                    </div>
                </div>
                <hr>
                <div class=" row">
                    <div class="col-sm-8">
                        <h6 class="mb-0">physical Activity Minutes:</h6>
                    </div>
                    <div class="col-sm-3 text-secondary">
                        <%if(result[0].totalphysicalActivityMinutes == null) {%>
                            <%= 0 %>
                        <%}
                        else {%>
                            <%= result[0].totalphysicalActivityMinutes %>
                        <%}%>
                    </div>
                </div>
               
            </div>
        </div>
    </div>

</div>
