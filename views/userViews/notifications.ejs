<div class="app-main__inner">

    <div class="table-title">
        <div class="row">
            <div class="col-sm-6">
                <h2><b>Announcements</b></h2>
            </div>
        </div>
    </div>
    <br>

    <div class="table-body">
        <% for (var i=0; i < result.length;i++) { %>
            <div class="toast-header">
                <svg class=" rounded mr-2" width="10" height="10" xmlns="http://www.w3.org/2000/svg"
                    preserveAspectRatio="xMidYMid slice" focusable="false" role="img">
                    <rect fill="#007aff" width="100%" height="100%" />
                </svg>
                <strong class="mr-auto">
                    <%= result[i].title%>
                </strong>
                <strong class="text-muted">
                    <%= result[i].msgDate%>
                </strong>
                <!-- <form action="/deleteAnnouncement" method="post">
                    <input type="submit" value="Dismiss" id="PopoverCustomT-1" class="btn btn-sm btn-md btn-danger">
                     <button type="button" class="ml-2 mb-1 close" data-dismiss="toast" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button> 
                </form> -->
            </div>
            <div class="toast-body">
                <%= result[i].message%>
            </div>
            <br>
            <% } %>
    </div>
</div>